<?xml version="1.0"?>
<robot name="bookle" xmlns:xacro="http://www.ros.org/wiki/xacro">

	<!-- Constants for robot dimensions -->
	<xacro:property name="PI" value="3.1415926535897931"/>

	<!-- Import all Gazebo-customization elements, including Gazebo colors -->
	<xacro:include filename="$(find bookle_description)/urdf/bookle.gazebo" />

	<link name="chassis">
		<collision name="collision">
			<origin xyz="0 0 0.1" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.15" length="0.003" />
			</geometry>
		</collision>

		<visual name="visual">
			<origin xyz="0 0 0.1" rpy="0 0 0" />
			<geometry>
				<cylinder radius="0.15" length="0.003" />
			</geometry>
		</visual>

		<collision name="front_caster_collision">
			<origin xyz="0.1 0 0.05" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.05" length="0.015" />
			</geometry>
		</collision>

		<visual name="front_caster_visual">
			<origin xyz="0.1 0 0.05" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.05" length="0.015" />
			</geometry>
		</visual>

		<collision name="back_caster_collision">
			<origin xyz="-0.1 0 0.05" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.05" length="0.015" />
			</geometry>
		</collision>

		<visual name="back_caster_visual">
			<origin xyz="-0.1 0 0.05" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.05" length="0.015" />
			</geometry>
		</visual>
	</link>

	<link name="left_wheel">
		<collision name="collision">
			<origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.04" length="0.015" />
			</geometry>
		</collision>
		<visual name="visual">
			<origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.04" length="0.015" />
			</geometry>
		</visual>
		<inertial>
			<mass value="1" />
			<origin xyz="0 0 1" rpy="0 0 0"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
		</inertial>
	</link>

	<link name="right_wheel">
		<collision name="collision">
			<origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.04" length="0.015" />
			</geometry>
		</collision>
		<visual name="visual">
			<origin xyz="0 0 0" rpy="0 1.5707 1.5707" />
			<geometry>
				<cylinder radius="0.04" length="0.015" />
			</geometry>
		</visual>
		<inertial>
			<mass value="1" />
			<origin xyz="0 0 1" rpy="0 0 0"/>
			<inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1" />
		</inertial>
	</link>

	<link name="left_frame">
		<!-- <origin xyz="0 -0.1 0.1" rpy="0 0 0" /> -->
		<collision name="left_collision">
			<origin xyz="0 0.0075 0" rpy="0 0 0" />
			<geometry>
				<box size=".08 .002 .06" />
			</geometry>
		</collision>
		<visual name="left_visual">
			<origin xyz="0 0.0075 0" rpy="0 0 0" />
			<geometry>
				<box size=".08 .002 .06" />
			</geometry>
		</visual>

		<collision name="right_collision">
			<origin xya="0 -0.0075 0" rpy="0 0 0"/>
			<geometry>
				<box size=".08 .002 .06" />
			</geometry>
		</collision>
		<visual name="right_visual">
			<origin xyz="0 -0.0075 0" rpy="0 0 0" />
			<geometry>
				<box size=".08 .002 .06" />
			</geometry>
		</visual>
	</link>

	<link name="right_frame">
		<!-- <origin xyz="0 0.1 0.1" rpy="0 0 0" /> -->
		<collision name="left_collision">
			<origin xyz="0 .0075 0" rpy="0 0 0" />
			<geometry>
				<box size=".08 .002 .06" />
			</geometry>
		</collision>
		<visual name="left_visual">
			<origin xyz="0 .0075 0" rpy="0 0 0" />
			<geometry>
				<box size=".08 .002 .06" />
			</geometry>
		</visual>

		<collision name="right_collision">
			<origin xyz="0 -0.0075 0" rpy="0 0 0" />
			<geometry>
				<box size=".08 .002 .06" />
			</geometry>
		</collision>
		<visual name="right_visual">
			<origin xyz="0 -0.0075 0" rpy="0 0 0" />
			<geometry>
				<box size=".08 .002 .06" />
			</geometry>
		</visual>
	</link>

	<joint type="revolute" name="left_wheel_hinge">
		<origin xyz="0 0 -0.03" rpy="0 0 0" />
		<child link="left_wheel" />
		<parent link="left_frame" />
		<axis xyz="0 1 0" />
		<limit effort="30" velocity="0.5" lower="-3.14" upper="3.14"/> 
	</joint>

	<joint type="revolute" name="right_wheel_hinge">
		<origin xyz="0 0 -0.03" rpy="0 0 0" />
		<child link="right_wheel" />
		<parent link="right_frame" />
		<axis xyz="0 1 0" />
		<limit effort="30" velocity="0.5" lower="-3.14" upper="3.14"/> 
	</joint>

	<joint type="fixed" name="left_frame_joint">
		<origin xyz="0 0.1 0.069" rpy="0 0 0" />
		<child link="left_frame" />
		<parent link="chassis" />
		<axis xyz="1 0 0" />
	</joint>

	<joint type="fixed" name="right_frame_joint">
		<origin xyz="0 -0.1 0.069" rpy="0 0 0" />
		<child link="right_frame" />
		<parent link="chassis" />
		<axis xyz="1 0 0" />
	</joint>


	<!-- <include>
		<uri>model://hokuyo</uri>
		<pose>0.1 0 0.135 0 0 0</pose>
	</include>
	<joint name="hokuyo_joint" type="revolute">
		<child>hokuyo::link</child>
		<parent>chassis</parent>
		<axis>
			<xyz>0 0 1</xyz>
			<limit>
				<upper>0</upper>
				<lower>0</lower>
			</limit>
		</axis>
	</joint> -->

	<joint name="hokuyo_joint" type="fixed">
		<axis xyz="0 1 0" />
		<origin xyz="0.1 0 0.135" rpy="0 0 0"/>
		<parent link="chassis"/>
		<child link="hokuyo_link"/>
	</joint>

	<!-- Hokuyo Laser -->
	<link name="hokuyo_link">
		<collision>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<box size="0.1 0.1 0.1"/>
			</geometry>
		</collision>

		<visual>
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<geometry>
				<mesh filename="package://bookle_description/meshes/hokuyo.dae"/>
			</geometry>
		</visual>

		<inertial>
			<mass value="1e-5" />
			<origin xyz="0 0 0" rpy="0 0 0"/>
			<inertia ixx="1e-6" ixy="0" ixz="0" iyy="1e-6" iyz="0" izz="1e-6" />
		</inertial>
	</link>

</robot>
